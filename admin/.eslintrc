{
  "root": true,
  "plugins": [
    "react",
    "tailwindcss",
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:react/recommended",
    "plugin:tailwindcss/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "tailwindcss/classname-order": "off"
  }
}
